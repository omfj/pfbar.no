{
  "name": "pink-flamingo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p *:dev",
    "web:dev": "next dev",
    "web:build": "next build",
    "web:start": "next start",
    "lint": "next lint",
    "db:dev": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "tsx drizzle/migrate.ts",
    "db:seed": "tsx drizzle/seed.ts",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "pages:watch": "npx @cloudflare/next-on-pages --watch",
    "pages:start": "npx wrangler pages dev .vercel/output/static --compatibility-date=2023-10-30 --compatibility-flag=nodejs_compat"
  },
  "dependencies": {
    "@libsql/client": "0.4.0-pre.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "drizzle-orm": "^0.29.0",
    "lucide-react": "^0.293.0",
    "nanoid": "^5.0.3",
    "next": "14.0.3",
    "next-auth": "^4.24.5",
    "pg": "^8.11.3",
    "postgres": "^3.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "server-only": "^0.0.1",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.7.4",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "20.9.1",
    "@types/pg": "^8.10.9",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "autoprefixer": "10.4.16",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.20.4",
    "eslint": "8.53.0",
    "eslint-config-next": "14.0.3",
    "eslint-plugin-next-on-pages": "^1.7.4",
    "npm-run-all": "^4.1.5",
    "postcss": "8.4.31",
    "tailwindcss": "3.3.5",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.5.0",
    "typescript": "5.2.2",
    "vercel": "^32.5.6"
  }
}